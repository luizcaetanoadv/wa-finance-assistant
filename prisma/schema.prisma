generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
url = env("DATABASE_URL")
}

model GoogleAuth {
  id           String   @id @default(cuid())
  userKey      String   @unique
  accessToken  String
  refreshToken String?
  scope        String?
  tokenType    String?
  expiryDate   BigInt?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
